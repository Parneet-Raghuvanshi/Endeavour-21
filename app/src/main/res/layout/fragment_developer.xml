<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="#fff"
    android:layout_height="match_parent"
    tools:context=".team.Developer">

    <!--( TOP BAR )-->
    <RelativeLayout
        android:id="@+id/top_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:orientation="horizontal">

        <ImageView
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/back_btn"
            android:src="@drawable/ic_main_arrow_back_ios_new_24"/>

        <TextView
            android:layout_marginTop="5dp"
            android:textAlignment="center"
            android:layout_alignParentTop="true"
            android:textStyle="bold"
            android:textColor="@color/main_color"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="Developers"
            android:layout_marginBottom="16dp"
            android:textSize="32sp" />

    </RelativeLayout>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/top_bar">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/top_bar">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:id="@+id/card_layout">

                <androidx.cardview.widget.CardView
                    android:layout_marginRight="30dp"
                    android:layout_marginLeft="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="10dp"
                    card_view:cardUseCompatPadding="true"
                    android:layout_marginTop="30dp"
                    android:layout_below="@+id/top_bar"
                    app:cardElevation="6dp">

                    <LinearLayout
                        android:weightSum="2"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:layout_height="match_parent">

                        <RelativeLayout
                            android:id="@+id/layout_team_card"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="#fff"
                            android:fontFamily="@font/nunito"
                            android:paddingHorizontal="20dp"
                            android:paddingVertical="12dp">

                            <LinearLayout
                                android:layout_marginBottom="10dp"
                                android:gravity="center"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="60dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:weightSum="3">

                                <TextView
                                    android:id="@+id/tv_team_name"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:textColor="@color/main_color"
                                    android:text="Nipun Kumar Drayan"
                                    android:textSize="20sp" />

                                <TextView
                                    android:id="@+id/tv_team_domain"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="8dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/nunito"
                                    android:gravity="center"
                                    android:text="TECHNICAL"
                                    android:textColor="@color/main_grey"
                                    android:textSize="15sp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/tv_team_desig"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="6dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/nunito"
                                    android:gravity="center"
                                    android:text="Head"
                                    android:textColor="@color/main_grey"
                                    android:textSize="15sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                        </RelativeLayout>

                    </LinearLayout>
                    <!--( BORDER )-->
                    <FrameLayout
                        android:background="@drawable/card_boarder"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                    <!--'( BORDER_END )-->
                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:gravity="center_horizontal"
                android:layout_marginTop="1dp">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:src="@drawable/nipun_imgteam"
                    android:id="@+id/image_team"
                    android:elevation="3dp"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_centerHorizontal="true"
                    android:padding="2dp"/>

            </LinearLayout>

            <!--( END )-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:id="@+id/card_layout1"
                android:layout_below="@+id/card_layout">

                <androidx.cardview.widget.CardView
                    android:layout_marginRight="30dp"
                    android:layout_marginLeft="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="10dp"
                    android:elevation="8dp"
                    android:layout_marginTop="30dp"
                    card_view:cardUseCompatPadding="true"
                    app:cardElevation="6dp">

                    <LinearLayout
                        android:weightSum="2"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:layout_height="match_parent">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="#fff"
                            android:fontFamily="@font/nunito"
                            android:paddingHorizontal="20dp"
                            android:paddingVertical="12dp">

                            <LinearLayout
                                android:layout_marginBottom="10dp"
                                android:gravity="center"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="60dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:weightSum="3">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:textColor="@color/main_color"
                                    android:text="Ritik Mehta"
                                    android:textSize="20sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="8dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/nunito"
                                    android:gravity="center"
                                    android:text="TECHNICAL"
                                    android:textColor="@color/main_grey"
                                    android:textSize="15sp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="6dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/nunito"
                                    android:gravity="center"
                                    android:text="Co-Head"
                                    android:textColor="@color/main_grey"
                                    android:textSize="15sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                        </RelativeLayout>

                    </LinearLayout>

                    <!--( BORDER )-->
                    <FrameLayout
                        android:background="@drawable/card_boarder"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                    <!--( BORDER_END )-->

                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:gravity="center_horizontal"
                android:layout_marginTop="1dp"
                android:layout_below="@+id/card_layout">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:src="@drawable/ritik_imgteam"
                    android:elevation="3dp"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_centerHorizontal="true"
                    android:padding="2dp"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/card_layout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:layout_below="@+id/card_layout1"
                android:layout_marginBottom="10dp">

                <androidx.cardview.widget.CardView
                    android:layout_marginRight="30dp"
                    android:layout_marginLeft="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="10dp"
                    android:elevation="8dp"
                    android:layout_marginTop="30dp"
                    card_view:cardUseCompatPadding="true"
                    android:layout_below="@+id/top_bar"
                    app:cardElevation="6dp">

                    <LinearLayout
                        android:weightSum="2"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:layout_height="match_parent">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="#fff"
                            android:fontFamily="@font/nunito"
                            android:paddingHorizontal="20dp"
                            android:paddingVertical="12dp">

                            <LinearLayout
                                android:layout_marginBottom="10dp"
                                android:gravity="center"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="60dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:weightSum="3">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:textColor="@color/main_color"
                                    android:text="Parneet Raghuvanshi"
                                    android:textSize="20sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="8dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/nunito"
                                    android:gravity="center"
                                    android:text="TECHNICAL"
                                    android:textColor="@color/main_grey"
                                    android:textSize="15sp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="6dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/nunito"
                                    android:gravity="center"
                                    android:text="Member"
                                    android:textColor="@color/main_grey"
                                    android:textSize="15sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                        </RelativeLayout>

                    </LinearLayout>

                    <!--( BORDER )-->
                    <FrameLayout
                        android:background="@drawable/card_boarder"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                    <!--( BORDER_END )-->
                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:gravity="center_horizontal"
                android:layout_marginTop="1dp"
                android:layout_below="@+id/card_layout1">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:src="@drawable/parneet_imgteam"
                    android:elevation="3dp"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_centerHorizontal="true"
                    android:padding="2dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:layout_below="@+id/card_layout2"
                android:layout_marginBottom="10dp">

                <androidx.cardview.widget.CardView
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginLeft="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="10dp"
                    android:elevation="8dp"
                    android:layout_marginTop="30dp"
                    card_view:cardUseCompatPadding="true"
                    android:layout_below="@+id/top_bar"
                    app:cardElevation="6dp">

                    <LinearLayout
                        android:weightSum="2"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:layout_height="match_parent">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="#fff"
                            android:fontFamily="@font/nunito"
                            android:paddingHorizontal="20dp"
                            android:paddingVertical="12dp">

                            <LinearLayout
                                android:layout_marginBottom="10dp"
                                android:gravity="center"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="60dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:weightSum="3">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:textColor="@color/main_color"
                                    android:text="Shivendu Mishra"
                                    android:textSize="20sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="8dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/nunito"
                                    android:gravity="center"
                                    android:text="TECHNICAL"
                                    android:textColor="@color/main_grey"
                                    android:textSize="15sp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="6dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/nunito"
                                    android:gravity="center"
                                    android:text="Member"
                                    android:textColor="@color/main_grey"
                                    android:textSize="15sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                        </RelativeLayout>

                    </LinearLayout>

                    <!--( BORDER )-->
                    <FrameLayout
                        android:background="@drawable/card_boarder"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                    <!--( BORDER_END )-->
                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:gravity="center_horizontal"
                android:layout_marginTop="1dp"
                android:layout_below="@+id/card_layout2">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:src="@drawable/shivendu_profile"
                    android:elevation="3dp"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_centerHorizontal="true"
                    android:padding="2dp"/>

            </LinearLayout>

        </RelativeLayout>
        <!--( END )-->
    </androidx.core.widget.NestedScrollView>

</RelativeLayout>